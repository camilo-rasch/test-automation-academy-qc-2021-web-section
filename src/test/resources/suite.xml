<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Final Exam Suite" verbose="1">
	<listeners>
		<listener class-name="com.web.automation.listener.Listener"/>
	</listeners>

	<parameter name="browser" value="chrome"/>
	<parameter name="url" value="https://www.espnqa.com/?src=com&amp;_adblock=true&amp;espn=cloud"/>

	<test name="Log In ESPN">
		<classes>
			<class name="com.web.automation.test.LogInTest"></class>
		</classes>
	</test>
	<test name="Log out ESPN">
		<classes>
			<class name="com.web.automation.test.LogOutTest"></class>
		</classes>
	</test>
	<test name="Cancel ESPN Account">
		<classes>
			<class name="com.web.automation.test.CancelAccountTest"></class>
		</classes>
	</test>

</suite>
