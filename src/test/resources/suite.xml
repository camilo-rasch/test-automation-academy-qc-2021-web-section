<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Select option in the dropdown" verbose="1">
	<listeners>
		<listener class-name="com.web.automation.listener.Listener"/>
	</listeners>

	<parameter name="browser" value="chrome"/>
	<parameter name="url" value="https://www.seleniumeasy.com/test/basic-select-dropdown-demo.html"/>

	<test name="Select Option">
		<classes>
			<class name="com.web.automation.test.SelectOptionTest"></class>
		</classes>
	</test>
</suite>
