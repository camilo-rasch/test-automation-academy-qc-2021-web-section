<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="example test suite" verbose="1">
	<listeners>
		<listener class-name="listener.Listener"/>
	</listeners>

	<test name="In User">
		<classes>
			<class name="com.automation.web.tests.InUserSuite"/>
		</classes>
	</test>
	<test name="Delete friend">
		<classes>
			<class name="com.automation.web.tests.DeleteFriend"/>
		</classes>
	</test>
	<test name="Out User">
		<classes>
			<class name="com.automation.web.tests.OutUserSuite"/>
		</classes>
	</test>
</suite>
