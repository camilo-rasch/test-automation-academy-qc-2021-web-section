<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="You Tube Iframe Test" verbose="1">
	<listeners>
		<listener class-name="com.web.automation.listener.Listener"/>
	</listeners>

	<parameter name = "browser" value = "chrome"/>
	<parameter name = "url" value = "https://mdbootstrap.com/docs/b4/jquery/plugins/iframe/"/>

	<test name="IFrameTest">
		<classes>
			<class name="com.web.automation.test.IFrameTest"></class>
		</classes>
	</test>
</suite>
